    <style>
        .checkbox-container {
            position: relative;
        }

            .checkbox-container input[type="checkbox"] {
                display: none;
            }

        .checkmark {
            position: relative;
            cursor: pointer;
        }

            .checkmark i {
                position: absolute;
                top: 0;
                left: 0;
                opacity: 0;
                transition: opacity 0.2s ease-in-out;
            }

        .checked-icon {
            opacity: 1;
        }

        .unchecked-icon {
            opacity: 0;
        }

        .checkbox-container input[type="checkbox"]:checked + .checkmark .checked-icon {
            opacity: 1;
        }

        .checkbox-container input[type="checkbox"]:checked + .checkmark .unchecked-icon {
            opacity: 0;
        }

