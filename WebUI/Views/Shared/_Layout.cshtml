    <script>
        function showFileName(input) {
            const fileName = input.files[0].name;
            const fileType = input.files[0].type;
            const fileLabel = input.parentElement.querySelector('.js-value');
            const uploadStatus = input.parentElement.nextElementSibling;
            const uploadProgressBar = uploadStatus.querySelector('.upload-progress');
            const fileInfo = uploadStatus.querySelector('.file-info');

            fileLabel.textContent = fileName;
            fileLabel.classList.add('is-selected');
            uploadStatus.style.display = 'block';
            fileInfo.textContent = `Dosya Türü: ${fileType}`;

            // Dosya yükleme simülasyonu
            let progress = 0;
            const interval = setInterval(function () {
                progress += 1;
                uploadProgressBar.style.width = `${progress}%`;

                if (progress >= 100) {
                    clearInterval(interval);
                    uploadStatus.textContent = 'Dosya yüklendi.';
                }
            }, 50); // Her 50ms'de bir güncelleme yap
        }


    </script>

    <script>
        function getFilePath() {
            var fileInput = document.getElementById("fileInput");
            var filePath = fileInput.value;

            if (filePath) {
                var filePathElement = document.getElementById("filePath");
                filePathElement.textContent = "Selected File Path: " + filePath;
            } else {
                alert("Please select a file.");
