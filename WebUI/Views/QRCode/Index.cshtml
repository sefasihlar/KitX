@{
    ViewData["Title"] = "Index";
}


<div id="content" class="main-content">

    <br />
    <br />
    <br />
    <br />
    <form asp-controller="QRCode" asp-action="CreateQrCode" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="length">Number of QR Codes:</label>
            <input type="number" name="length" id="length" required />
        </div>
        <button type="submit" class="btn btn-primary">Generate QR Codes</button>
    </form>

</div>
   



